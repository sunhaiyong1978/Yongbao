source env/target_base/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=target_base
export STEP_PACKAGENAME=man-pages
export PACKAGE_VERSION=6.8
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/target_base/man-pages/6.8/
rm -rf ${BUILD_DIRECTORY}/man-pages-6.8
tar xvf ${DOWNLOADDIR}/man-pages-6.8.tar.xz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/man-pages-6.8
	make prefix=${PREFIX} DESTDIR=${SYSROOT_DIR} install
popd 
rm -rf ${BUILD_DIRECTORY}/man-pages-6.8
