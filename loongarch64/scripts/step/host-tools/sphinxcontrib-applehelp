source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=sphinxcontrib-applehelp
export PACKAGE_VERSION=1.0.8
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/sphinxcontrib-applehelp/1.0.8/
rm -rf ${BUILD_DIRECTORY}/sphinxcontrib_applehelp-1.0.8
tar xvf ${DOWNLOADDIR}/sphinxcontrib_applehelp-1.0.8.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/sphinxcontrib_applehelp-1.0.8
	
	${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user sphinxcontrib-applehelp
popd 
rm -rf ${BUILD_DIRECTORY}/sphinxcontrib_applehelp-1.0.8
