source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=requests
export PACKAGE_VERSION=2.32.5
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/requests/2.32.5/
rm -rf ${BUILD_DIRECTORY}/requests-2.32.5
tar xvf ${DOWNLOADDIR}/requests-2.32.5.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/requests-2.32.5
	
	${HOST_TOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${HOST_TOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user requests
popd 
rm -rf ${BUILD_DIRECTORY}/requests-2.32.5
