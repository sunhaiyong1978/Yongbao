source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=alabaster
export PACKAGE_VERSION=0.7.13
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/alabaster/0.7.13/
rm -rf ${BUILD_DIRECTORY}/alabaster-0.7.13
tar xvf ${DOWNLOADDIR}/alabaster-0.7.13.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/alabaster-0.7.13
	
	${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user alabaster
popd 
rm -rf ${BUILD_DIRECTORY}/alabaster-0.7.13
