source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=attrs
export PACKAGE_VERSION=24.2.0
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/attrs/24.2.0/
rm -rf ${BUILD_DIRECTORY}/attrs-24.2.0
tar xvf ${DOWNLOADDIR}/attrs-24.2.0.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/attrs-24.2.0
	
	${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user attrs
popd 
rm -rf ${BUILD_DIRECTORY}/attrs-24.2.0
