source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=MarkupSafe
export PACKAGE_VERSION=2.1.2
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/MarkupSafe/2.1.2/
rm -rf ${BUILD_DIRECTORY}/MarkupSafe-2.1.2
tar xvf ${DOWNLOADDIR}/MarkupSafe-2.1.2.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/MarkupSafe-2.1.2
	${CROSSTOOLS_DIR}/bin/python3 setup.py build
        ${CROSSTOOLS_DIR}/bin/python3 setup.py install --optimize=1
popd 
rm -rf ${BUILD_DIRECTORY}/MarkupSafe-2.1.2
