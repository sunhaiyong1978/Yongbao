source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=MarkupSafe
export PACKAGE_VERSION=3.0.3
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/MarkupSafe/3.0.3/
rm -rf ${BUILD_DIRECTORY}/markupsafe-3.0.3
tar xvf ${DOWNLOADDIR}/markupsafe-3.0.3.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/markupsafe-3.0.3
	
	${HOST_TOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${HOST_TOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user MarkupSafe
popd 
rm -rf ${BUILD_DIRECTORY}/markupsafe-3.0.3
