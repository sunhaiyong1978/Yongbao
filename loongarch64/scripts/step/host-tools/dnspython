source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=dnspython
export PACKAGE_VERSION=2.4.2
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/dnspython/2.4.2/
rm -rf ${BUILD_DIRECTORY}/dnspython-2.4.2
tar xvf ${DOWNLOADDIR}/dnspython-2.4.2.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/dnspython-2.4.2
	
	${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user dnspython
popd 
rm -rf ${BUILD_DIRECTORY}/dnspython-2.4.2
