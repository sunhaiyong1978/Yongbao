source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=pycparser
export PACKAGE_VERSION=2.23
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/pycparser/2.23/
rm -rf ${BUILD_DIRECTORY}/pycparser-2.23
tar xvf ${DOWNLOADDIR}/pycparser-2.23.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/pycparser-2.23
	
	${HOST_TOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        ${HOST_TOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --force-reinstall --no-user pycparser
popd 
rm -rf ${BUILD_DIRECTORY}/pycparser-2.23
