source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=setuptools_pre
export PACKAGE_VERSION=69.0.3
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/setuptools_pre/69.0.3/
rm -rf ${BUILD_DIRECTORY}/setuptools-69.0.3
tar xvf ${DOWNLOADDIR}/setuptools-69.0.3.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/setuptools-69.0.3
	${CROSSTOOLS_DIR}/bin/python3 setup.py build
	${CROSSTOOLS_DIR}/bin/python3 setup.py install
popd 
rm -rf ${BUILD_DIRECTORY}/setuptools-69.0.3
