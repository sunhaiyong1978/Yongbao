source env/host-tools/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=host-tools
export STEP_PACKAGENAME=setuptools_pre
export PACKAGE_VERSION=70.1.1
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/host-tools/setuptools_pre/70.1.1/
rm -rf ${BUILD_DIRECTORY}/setuptools-70.1.1
tar xvf ${DOWNLOADDIR}/setuptools-70.1.1.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/setuptools-70.1.1
	${CROSSTOOLS_DIR}/bin/python3 setup.py build
	${CROSSTOOLS_DIR}/bin/python3 setup.py install
popd 
rm -rf ${BUILD_DIRECTORY}/setuptools-70.1.1
