source env/bootstrap/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=bootstrap
export STEP_PACKAGENAME=rust-bootstrap
export PACKAGE_VERSION=1.73.0-default
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/bootstrap/rust-bootstrap/1.73.0-default/
rm -rf ${BUILD_DIRECTORY}/rust-1.73.0-x86_64-unknown-linux-gnu
tar xvf ${DOWNLOADDIR}/rust-1.73.0-x86_64-unknown-linux-gnu.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/rust-1.73.0-x86_64-unknown-linux-gnu
./install.sh --destdir=${CROSSTOOLS_DIR}/rust
popd 
rm -rf ${BUILD_DIRECTORY}/rust-1.73.0-x86_64-unknown-linux-gnu
