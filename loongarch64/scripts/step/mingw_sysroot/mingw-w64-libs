source env/mingw_sysroot/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=mingw_sysroot
export STEP_PACKAGENAME=mingw-w64-libs
export PACKAGE_VERSION=v11.0.0
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/mingw_sysroot/mingw-w64-libs/v11.0.0/
rm -rf ${BUILD_DIRECTORY}/mingw-w64-v11.0.0
tar xvf ${DOWNLOADDIR}/mingw-w64-v11.0.0.tar.bz2 -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/mingw-w64-v11.0.0
	mkdir -p build
	pushd build
		../configure --host=${MINGW_HOST} --prefix=/mingw --without-headers --without-crt --with-libraries
		make -j${JOBS}
		make DESTDIR=${MINGW_SYSROOT_DIR} install
	popd
popd 
rm -rf ${BUILD_DIRECTORY}/mingw-w64-v11.0.0
