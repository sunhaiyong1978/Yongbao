source env/desktop-app/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=desktop-app
export STEP_PACKAGENAME=cssselect
export PACKAGE_VERSION=1.3.0
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/desktop-app/cssselect/1.3.0/
rm -rf ${BUILD_DIRECTORY}/cssselect-1.3.0
tar xvf ${DOWNLOADDIR}/cssselect-1.3.0.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/cssselect-1.3.0
	
	CC=${CROSS_TARGET}-gcc CXX=${CROSS_TARGET}-g++ _PYTHON_HOST_PLATFORM=linux-loongarch64 _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${HOST_TOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        CC=${CROSS_TARGET}-gcc CXX=${CROSS_TARGET}-g++ _PYTHON_HOST_PLATFORM=linux-loongarch64 _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${HOST_TOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user cssselect --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/cssselect-1.3.0
