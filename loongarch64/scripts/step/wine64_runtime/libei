source env/wine64_runtime/config
source env/distro.info
source env/function.sh
source env/wine64_runtime/custom
export STEP_BUILDNAME=wine64_runtime
export STEP_PACKAGENAME=libei
export PACKAGE_VERSION=1.2.1
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/wine64_runtime/libei/1.2.1/
rm -rf ${BUILD_DIRECTORY}/libei-1.2.1
tar xvf ${DOWNLOADDIR}/libei-1.2.1.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/libei-1.2.1
        

	tar xvf ${RESOURCEDIR}/files/libei-munit_git.tar.gz
	mv libei-munit_git  subprojects/munit

        mkdir cross-build
        pushd cross-build
                meson ${MESON_CONFIG} --default-library=both \
                      ..
                eval ${NINJA_AND_INSTALL}
        popd
        rm -f ${X86_64_SYSROOT_DIR}/usr/lib64/*.la
popd 
rm -rf ${BUILD_DIRECTORY}/libei-1.2.1
