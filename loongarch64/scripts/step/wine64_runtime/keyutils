source env/wine64_runtime/config
source env/distro.info
source env/function.sh
source env/wine64_runtime/custom
export STEP_BUILDNAME=wine64_runtime
export STEP_PACKAGENAME=keyutils
export PACKAGE_VERSION=1.6.1
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/wine64_runtime/keyutils/1.6.1/
rm -rf ${BUILD_DIRECTORY}/keyutils-1.6.1
tar xvf ${DOWNLOADDIR}/keyutils-1.6.1.tar.bz2 -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/keyutils-1.6.1
	patch -Np1 -i ${SYSDIR}/files/wine64_runtime/keyutils/1.6.1/patches/0001-keyutils-fix-for-cross-build.patch 
	 
	CC=${CROSS_TARGET}-gcc CXX=${CROSS_TARGET}-g++ make -j${JOBS}
	make NO_ARLIB=1 LIBDIR=/usr/lib${LIB_SUFF} BINDIR=/usr/bin SBINDIR=/usr/sbin DESTDIR=${X86_64_SYSROOT_DIR} install
popd 
rm -rf ${BUILD_DIRECTORY}/keyutils-1.6.1
