source env/wine64_runtime/config
source env/distro.info
source env/function.sh
source env/wine64_runtime/custom
export STEP_BUILDNAME=wine64_runtime
export STEP_PACKAGENAME=selinux
export PACKAGE_VERSION=3.5
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/wine64_runtime/selinux/3.5/
rm -rf ${BUILD_DIRECTORY}/selinux-3.5
tar xvf ${DOWNLOADDIR}/selinux-3.5.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/selinux-3.5
	pushd libselinux
		CC=${CROSS_TARGET}-gcc CXX=${CROSS_TARGET}-g++ make -j${JOBS}
		CC=${CROSS_TARGET}-gcc CXX=${CROSS_TARGET}-g++ make DESTDIR=${X86_64_SYSROOT_DIR} install
	popd
popd 
rm -rf ${BUILD_DIRECTORY}/selinux-3.5
