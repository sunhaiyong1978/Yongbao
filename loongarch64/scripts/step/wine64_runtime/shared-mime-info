source env/wine64_runtime/config
source env/distro.info
source env/function.sh
source env/wine64_runtime/custom
export STEP_BUILDNAME=wine64_runtime
export STEP_PACKAGENAME=shared-mime-info
export PACKAGE_VERSION=2.3
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/wine64_runtime/shared-mime-info/2.3/
rm -rf ${BUILD_DIRECTORY}/shared-mime-info-2.3
tar xvf ${DOWNLOADDIR}/shared-mime-info-2.3.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/shared-mime-info-2.3
	
	mkdir cross-build
	pushd cross-build
		meson ${MESON_CONFIG} --default-library=both  \
                      ..
		eval ${NINJA_AND_INSTALL}
	popd
	rm -f ${X86_64_SYSROOT_DIR}/usr/lib64/*.la
popd 
rm -rf ${BUILD_DIRECTORY}/shared-mime-info-2.3
