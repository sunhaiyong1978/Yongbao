source env/wine64_runtime/config
source env/distro.info
source env/function.sh
source env/wine64_runtime/custom
export STEP_BUILDNAME=wine64_runtime
export STEP_PACKAGENAME=gsettings-desktop-schemas
export PACKAGE_VERSION=46.0
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/wine64_runtime/gsettings-desktop-schemas/46.0/
rm -rf ${BUILD_DIRECTORY}/gsettings-desktop-schemas-46.0
tar xvf ${DOWNLOADDIR}/gsettings-desktop-schemas-46.0.tar.xz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/gsettings-desktop-schemas-46.0
	
	mkdir cross-build
	pushd cross-build
		 meson ${MESON_CONFIG} --default-library=both -Dintrospection=false \
                      ..
		eval ${NINJA_AND_INSTALL}
	popd
	rm -f ${X86_64_SYSROOT_DIR}/usr/lib64/*.la
popd 
rm -rf ${BUILD_DIRECTORY}/gsettings-desktop-schemas-46.0
