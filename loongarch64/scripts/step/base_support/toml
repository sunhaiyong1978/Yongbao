source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=toml
export PACKAGE_VERSION=0.10.2
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/toml/0.10.2/
rm -rf ${BUILD_DIRECTORY}/toml-0.10.2
tar xvf ${DOWNLOADDIR}/toml-0.10.2.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/toml-0.10.2
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/python3 setup.py build
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/python3 setup.py install --optimize=1 --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/toml-0.10.2
