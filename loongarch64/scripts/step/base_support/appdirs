source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=appdirs
export PACKAGE_VERSION=1.4.4
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/appdirs/1.4.4/
rm -rf ${BUILD_DIRECTORY}/appdirs-1.4.4
tar xvf ${DOWNLOADDIR}/appdirs-1.4.4.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/appdirs-1.4.4
	
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user appdirs --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/appdirs-1.4.4
