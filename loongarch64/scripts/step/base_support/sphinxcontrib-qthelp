source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=sphinxcontrib-qthelp
export PACKAGE_VERSION=1.0.3
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/sphinxcontrib-qthelp/1.0.3/
rm -rf ${BUILD_DIRECTORY}/sphinxcontrib-qthelp-1.0.3
tar xvf ${DOWNLOADDIR}/sphinxcontrib-qthelp-1.0.3.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/sphinxcontrib-qthelp-1.0.3
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user sphinxcontrib-qthelp --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/sphinxcontrib-qthelp-1.0.3
