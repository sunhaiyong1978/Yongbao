source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=jsonschema
export PACKAGE_VERSION=4.19.1
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/jsonschema/4.19.1/
rm -rf ${BUILD_DIRECTORY}/jsonschema-4.19.1
tar xvf ${DOWNLOADDIR}/jsonschema-4.19.1.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/jsonschema-4.19.1
	
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user jsonschema --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/jsonschema-4.19.1
