source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=trove-classifiers
export PACKAGE_VERSION=2024.7.2
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/trove-classifiers/2024.7.2/
rm -rf ${BUILD_DIRECTORY}/trove_classifiers-2024.7.2
tar xvf ${DOWNLOADDIR}/trove_classifiers-2024.7.2.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/trove_classifiers-2024.7.2
	
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user trove-classifiers --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/trove_classifiers-2024.7.2
