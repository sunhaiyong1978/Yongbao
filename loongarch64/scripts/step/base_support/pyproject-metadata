source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=pyproject-metadata
export PACKAGE_VERSION=0.9.0
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/pyproject-metadata/0.9.0/
rm -rf ${BUILD_DIRECTORY}/pyproject_metadata-0.9.0
tar xvf ${DOWNLOADDIR}/pyproject_metadata-0.9.0.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/pyproject_metadata-0.9.0
	
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${HOST_TOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${HOST_TOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user pyproject-metadata --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/pyproject_metadata-0.9.0
