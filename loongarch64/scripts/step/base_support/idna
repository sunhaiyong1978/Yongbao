source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=idna
export PACKAGE_VERSION=3.6
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/idna/3.6/
rm -rf ${BUILD_DIRECTORY}/idna-3.6
tar xvf ${DOWNLOADDIR}/idna-3.6.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/idna-3.6
	
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${CROSSTOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user idna --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/idna-3.6
