source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=numpy
export PACKAGE_VERSION=2.3.4
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/numpy/2.3.4/
rm -rf ${BUILD_DIRECTORY}/numpy-2.3.4
tar xvf ${DOWNLOADDIR}/numpy-2.3.4.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/numpy-2.3.4
        
	mkdir cross-build
	pushd cross-build
		../vendored-meson/meson/meson.py ${MESON_CONFIG} -Db_ndebug=if-release -Db_vscrt=md ..
		eval ${NINJA_AND_INSTALL}
	popd
	rm -f ${SYSROOT_DIR}/usr/lib${LIB_SUFF}*.la
popd 
rm -rf ${BUILD_DIRECTORY}/numpy-2.3.4
