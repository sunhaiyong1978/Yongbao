source env/base_support/config
source env/distro.info
source env/function.sh
export STEP_BUILDNAME=base_support
export STEP_PACKAGENAME=alabaster
export PACKAGE_VERSION=1.0.0
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/base_support/alabaster/1.0.0/
rm -rf ${BUILD_DIRECTORY}/alabaster-1.0.0
tar xvf ${DOWNLOADDIR}/alabaster-1.0.0.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/alabaster-1.0.0
	
	_PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${HOST_TOOLS_DIR}/bin/pip3 wheel -w dist --no-build-isolation --no-deps ${PWD}
        _PYTHON_SYSCONFIGDATA_NAME=_sysconfigdata__linux_${CROSS_TARGET} ${HOST_TOOLS_DIR}/bin/pip3 install --no-index --find-links dist --no-cache-dir --no-deps --ignore-installed --no-user alabaster --root=${SYSROOT_DIR} --prefix=/usr
popd 
rm -rf ${BUILD_DIRECTORY}/alabaster-1.0.0
