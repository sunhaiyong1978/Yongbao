source env/cross-tools/config
source env/distro.info
source env/function.sh
source env/cross-tools/custom
export STEP_BUILDNAME=cross-tools
export STEP_PACKAGENAME=URI
export PACKAGE_VERSION=5.26
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/cross-tools/URI/5.26/
rm -rf ${BUILD_DIRECTORY}/URI-5.26
tar xvf ${DOWNLOADDIR}/URI-5.26.tar.gz -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/URI-5.26
	
	${CROSSTOOLS_DIR}/bin/perl Makefile.PL
	eval ${MAKE_AND_INSTALL}
popd 
rm -rf ${BUILD_DIRECTORY}/URI-5.26
