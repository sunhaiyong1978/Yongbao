source env/linux_runtime/config
source env/distro.info
source env/function.sh
source env/linux_runtime/custom
export STEP_BUILDNAME=linux_runtime
export STEP_PACKAGENAME=libgpg-error
export PACKAGE_VERSION=1.51
export RESOURCEDIR=${NEW_TARGET_SYSDIR}/files/linux_runtime/libgpg-error/1.51/
rm -rf ${BUILD_DIRECTORY}/libgpg-error-1.51
tar xvf ${DOWNLOADDIR}/libgpg-error-1.51.tar.bz2 -C ${BUILD_DIRECTORY} 
pushd ${BUILD_DIRECTORY}/libgpg-error-1.51
    ./configure ${COMMON_CONFIG} --enable-install-gpg-error-config
    eval ${MAKE_AND_INSTALL}
popd 
rm -rf ${BUILD_DIRECTORY}/libgpg-error-1.51
